<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">


	<servlet>
		<servlet-name>AddServlet</servlet-name>
		<servlet-class>com.douglas254.AddServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AddServlet</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SqServlet</servlet-name>
		<servlet-class>com.douglas254.SqServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SqServlet</servlet-name>
		<url-pattern>/Sq</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>MyServlet</servlet-name>
		<servlet-class>com.douglas254.MyServlet</servlet-class>
		
		<init-param>
			<param-name>name</param-name>
			<param-value>Douglas Obara</param-value>
		</init-param>
		
	</servlet>

	<servlet-mapping>
		<servlet-name>MyServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>name</param-name>
		<param-value>Douglas</param-value>
	</context-param>

	<context-param>
		<param-name>phone</param-name>
		<param-value>Iphone</param-value>
	</context-param>

</web-app>